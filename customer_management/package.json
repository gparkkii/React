{
    "name": "management",
    "version": "1.0.0",
    "description": "'client' 폴더에서는 react를, 'management' 폴더에서는 node.js를 실행시키도록 명시해주는 package.json 파일입니다.",
    "scripts": {
        "client": "cd client && yarn start",
        "server": "nodemon server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
    },
    "dependencies": {
        "@material-ui/icons": "^4.11.2",
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "multer": "^1.4.2",
        "mysql": "^2.18.1"
    },
    "devDependencies": {
        "concurrently": "^5.3.0"
    }
}
